{
	"name": "laravel/laravel",
	"description": "The Laravel Framework.",
	"keywords": ["framework", "laravel"],
	"license": "MIT",
    "repositories": [
        {// Override for vespakoen/menu
            "type": "vcs",
            "url": "git://github.com/martynling/menu.git"
        }
    ],
	"require": {
		"laravel/framework": "4.2.*",
        "machuga/authority-l4": "dev-master",
        "magniloquent/magniloquent": "dev-master",
        "codesleeve/asset-pipeline": "dev-master",
		"coderabbi/virtuoso": "0.*",
		"davejamesmiller/laravel-breadcrumbs": "~2.2.1",
		"vespakoen/menu": "dev-master",
        "anahkiasen/former": "dev-master",
        "league/flysystem": "0.5.*",
        "aws/aws-sdk-php": "dev-master",
        "mjohnson/transit": "dev-master",
        "opentok/opentok": "dev-master#e991b877b44431b8dea7bd90287bb4030433e6c5"
	},
    "require-dev": {
        "way/generators": "2.*",
		"barryvdh/laravel-ide-helper": "1.*"
    },
    "autoload": {
		"classmap": [
			"app/commands",
            "app/composers",
			"app/controllers",
			"app/models",
			"app/database/migrations",
			"app/database/seeds",
			"app/tests/TestCase.php",
            "app/classes"
		]
	},
	"scripts": {
		"post-install-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-update-cmd": [
			"php artisan clear-compiled",
            "php artisan ide-helper:generate",
			"php artisan optimize"
		],
		"post-create-project-cmd": [
			"php artisan key:generate"
		]
	},
	"config": {
		"preferred-install": "dist"
	},
	"minimum-stability": "stable"
}
